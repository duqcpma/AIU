        if (jobGoals.size() != 0 && postsecGoals.size() != 0) {
            displayGoal.getItems().add(theSeparator);
        }
        displayGoal.getItems().addAll(postsecGoals);
        if (postsecGoals.size() != 0 && otherGoals.size() != 0) {
            displayGoal.getItems().add(theSeparator);
