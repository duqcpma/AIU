package aiumain;

/**
 *
 * @author Adam
 */
public class Node <K extends Comparable> {

    public K key;
    public Student data;
    public Node<K> left, right;

    public Node(K k, Student d, Node<K> l,Node <K> r) {
        key = k; data = d; left = l; right = r;
    }

    public Node(K k, Student d) {
        this(k,d,null,null);
    }
    
    public String toString() {
        return (String)key;
    }
}
