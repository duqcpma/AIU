try{
                String lname = "Last Name";
                String fname = "First Name";
                String phNo = "Phone Number";
                String email = "Email Address";
                
                File file = new File("C:\\Users\\"+System.getProperty("user.name")+"\\Desktop\\studentlist.xls");
                
                WritableWorkbook wworkbook;
      
                //setting the font,size,style,color for the cells
                WritableFont arialBold11font = new WritableFont(WritableFont.ARIAL, 12, WritableFont.BOLD,false,UnderlineStyle.SINGLE);
                WritableFont arial10font = new WritableFont(WritableFont.ARIAL,10);
                
                WritableCellFormat ab11format = new WritableCellFormat(arialBold11font);
                WritableCellFormat a10format = new WritableCellFormat(arial10font);
                WritableCellFormat a10formatGray = new WritableCellFormat(arial10font);
                
                a10formatGray.setBackground(Colour.GRAY_25);
                
                wworkbook = Workbook.createWorkbook(file);
      
                WritableSheet wsheet = wworkbook.createSheet("First Sheet", 0);
                
                AIUTreeIterator<String, ArrayList<Student>> it = new AIUTreeIterator(database);
                
                jxl.write.Label l1 = new jxl.write.Label(0,0,lname,ab11format);
                jxl.write.Label l2 = new jxl.write.Label(1,0,fname,ab11format);
                jxl.write.Label l3 = new jxl.write.Label(2,0,phNo,ab11format);
                jxl.write.Label l4 = new jxl.write.Label(3,0,email,ab11format);
                
                int i=1;

                
                while(it.hasNext()){
                    ArrayList<Student> currentstudent = it.next().data;
                    for(Student student : currentstudent) {
                        if(i%2 != 1){
                        jxl.write.Label l5 = new jxl.write.Label(0,i,student.lastName,a10format);
                        jxl.write.Label l6 = new jxl.write.Label(1,i,student.firstName,a10format);
                        jxl.write.Label l7 = new jxl.write.Label(2,i,student.phoneNumber,a10format);
                        jxl.write.Label l8 = new jxl.write.Label(3,i,student.emailAddress,a10format);
                        wsheet.addCell(l5);
                        wsheet.addCell(l6);
                        wsheet.addCell(l7);
                        wsheet.addCell(l8);
                        }//end if
                        else {
                        jxl.write.Label l5 = new jxl.write.Label(0,i,student.lastName,a10formatGray);
                        jxl.write.Label l6 = new jxl.write.Label(1,i,student.firstName,a10formatGray);
                        jxl.write.Label l7 = new jxl.write.Label(2,i,student.phoneNumber,a10formatGray);
                        jxl.write.Label l8 = new jxl.write.Label(3,i,student.emailAddress,a10formatGray);    
                        wsheet.addCell(l5);
                        wsheet.addCell(l6);
                        wsheet.addCell(l7);
                        wsheet.addCell(l8);
                        }
                        i++;
                          
                }//end for
                }//end while
                
                //sets the column width
                wsheet.setColumnView(0,16);
                wsheet.setColumnView(1,16);
                wsheet.setColumnView(2,18);
                wsheet.setColumnView(3,36);
      
                //adds the labels of the headings
                wsheet.addCell(l1);
                wsheet.addCell(l2);
                wsheet.addCell(l3);
                wsheet.addCell(l4);
                
                wworkbook.write();
                wworkbook.close();
                
                Desktop.getDesktop().open(file);
                }//end try
                catch(IOException e){
                    System.err.println("no bueno 1");    
                }//end catch 1
                catch(WriteException w){
                    System.err.println("no bueno 2");   
                }//end catch 2
